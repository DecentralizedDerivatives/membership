{
  "name": "DDA_membership",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "npm run clear-dist && npm run build && NODE_ENV=production node server.js",
    "build": "parcel build --no-cache index.html --public-url ./",
    "dev": "npm run clear-dist && standard && concurrently -k \"npm run dev-build\" \"npm run dev-server\"",
    "dev-build": "parcel watch index.html --public-url ./",
    "dev-server": "NODE_ENV=dev nodemon server.js",
    "clear-dist": "babel-node ./scripts/clearDistFolder.js"
  },
  "dependencies": {
    "@material-ui/core": "^1.2.0",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "express-winston": "^2.5.1",
    "helmet": "^3.12.1",
    "mongoose": "^5.1.4",
    "normalize": "^0.3.1",
    "normalize.css": "^8.0.0",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.2.2",
    "standard": "^11.0.1",
    "validator": "^10.3.0",
    "web3": "^1.0.0-beta.34",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chalk": "^2.4.1",
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.5",
    "parcel-bundler": "^1.8.1",
    "rimraf": "^2.6.2"
  },
  "engines": {
    "node": "10.3.0"
  }
}
