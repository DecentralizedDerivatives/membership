{
  "name": "DDA_membership",
  "version": "1.0.0",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "npm run clear-dist && npm run build && NODE_ENV=production node server.js",
    "build": "parcel build --no-cache index.html --public-url ./",
    "dev": "npm run clear-dist && concurrently -k \"npm run dev-build\" \"npm run dev-server\"",
    "dev-build": "parcel watch index.html --public-url ./",
    "dev-server": "NODE_ENV=dev nodemon server.js",
    "clear-dist": "babel-node ./scripts/clearDistFolder.js",
    "test": "jest --env=node"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.2",
    "@material-ui/icons": "^1.1.0",
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.6",
    "express": "^4.16.3",
    "express-winston": "^2.6.0",
    "helmet": "^3.13.0",
    "mongoose": "^5.2.6",
    "normalize": "^0.3.1",
    "normalize.css": "^8.0.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-router-dom": "^4.2.2",
    "validator": "^10.5.0",
    "web3": "1.0.0-beta.31",
    "web3-net": "^1.0.0-beta.35",
    "websocket": "^1.0.26",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "chalk": "^2.4.1",
    "concurrently": "^3.6.1",
    "enzyme": "*",
    "jest": "^23.4.2",
    "nodemon": "^1.18.3",
    "parcel-bundler": "^1.9.7",
    "react-addons-test-utils": "*",
    "rimraf": "^2.6.2",
    "standard": "^11.0.1"
  },
  "jest": {
    "verbose": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
